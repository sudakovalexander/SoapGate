<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://custsystems.com/integration/abs"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:bons0="http://custsystems.com/integration/abs"
            elementFormDefault="qualified">
	<xsd:complexType name="AccAccBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код запроса от системы
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование системы, сформировавшей запрос
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="CustomerID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор клиента
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element minOccurs="1" name="BranchId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор подразделения банка клиента (БИК или код)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="FlagComiss" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>Признак расчета комиссии операции. Принимает
						значения: 0 Не рассчитывать. 1. Рассчитывать.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="1" name="A" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CUR" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Символьный ISO-код валюты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CRD" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Цифровой ISO-код валюты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="D" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата первичного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="N" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер первичного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PACC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Счет, с которого переводят</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RACC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Счет, на который переводят</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PCARD" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер карты плательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RCARD" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер карты получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="COMISSION" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма комиссии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="COMISSIONACC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Счет для списания комиссии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DOCID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Референс платежного документа
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="GND" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Основание перевода</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PNAME" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>ФИО Клиента (полностью)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RNAME" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>ФИО получателя (полностью)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PINN" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>ИНН клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AccAccResponse">
		<xsd:sequence>
			<xsd:element minOccurs="1" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="1" name="CustomerID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="AnsDateTime" type="xsd:dateTime"/>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="BranchId" type="xsd:string"/>
			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="REF" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Ссылка на документ во внешней системе
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="Status" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Статус документа во внешней системе
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="AmmountComiss" type="xsd:decimal">
							<xsd:annotation>
								<xsd:documentation>Сумма рассчитанной комиссии
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="TransferCurr" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Валюта комиссии</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AccountListBO">
		<xsd:sequence>
			<xsd:element minOccurs="1" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="1" name="CustomerID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="BranchId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор подразделения банка клиента (БИК или код)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ShowRest" type="xsd:int"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AccountListResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BranchId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>

			<xsd:element name="ShowRest" type="xsd:int" minOccurs="0"/>
			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="accounts" type="bons0:Account" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AccBalBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys"
			             type="xsd:string">
			</xsd:element>
			<xsd:element minOccurs="0" name="CustomerID"
			             type="xsd:string">
			</xsd:element>

			<xsd:element minOccurs="0" name="N" type="xsd:string"><xsd:annotation><xsd:documentation>Номер счета, по которому запрашивается остаток</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="response"
			             type="bons0:AccBalResponse">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AccBalResponse">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="CUR" type="xsd:string"><xsd:annotation><xsd:documentation>Символьный ISO-код валюты счета</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Rest" type="xsd:decimal"><xsd:annotation><xsd:documentation>Остаток на счете</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="Account">
		<xsd:sequence>
			<xsd:element minOccurs="1" name="CA">
				<xsd:annotation>
					<xsd:documentation>
						Номер счёта
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CUR">
				<xsd:annotation>
					<xsd:documentation>
						Символьный ISO-код валюты счета.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element minOccurs="0" name="CRD">
				<xsd:annotation>
					<xsd:documentation>
						Цифровой ISO-код валюты счета.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element minOccurs="0" name="BIC">
				<xsd:annotation>
					<xsd:documentation>
						БИК филиала Банка, где открыт счет клиента.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="9" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element minOccurs="1" name="NAME" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Название счета.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="1" name="TYPE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Тип счета.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="1" name="SUBTYPE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Подтип счета.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="REST" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Остаток по счету.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="PLANREST" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Остаток по счету.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="DO" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата открытия счета.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="ED" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата окончания договора.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="EDA" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата окончания срока, допускающего пополнение.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="BDW" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата, раньше которой нельзя пополнять депозит.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="LIMIT" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Неснижаемый остаток по счету.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="MA" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Минимальная сумма пополнения счета.
					</xsd:documentation>
				</xsd:annotation>

			</xsd:element>

			<xsd:element minOccurs="0" name="PERCENT" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Процентная ставка по депозиту.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="ADPERC" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма начисленных процентов по депозиту.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="FILIAL" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Код филиала банка, где открыт счет клиента.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="STATUS" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Статус счета
						Принимает значения: 0=действует
						1=блокирован
						2=закрыт
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="RESTBS" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Исходящий остаток по счету на текущую дату в валюте ЦБ
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CancelBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string">
			</xsd:element>
			<xsd:element minOccurs="0" name="CustomerID" type="xsd:string">
			</xsd:element>
			<xsd:element minOccurs="0" name="CancelDocRef" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Референс отзываемого документа
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="D" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="N" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер платежного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CANCELDOCID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип отзываемого документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="BranchId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Идентификатор филиала
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="Amount" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма транзакции
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="TransactionCur" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Буквенный код валюты транзакции
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="CardNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Номер карты клиента
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CancelResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>
			<!-- Хлынов -->
			<xsd:element name="BranchId" type="xsd:string" minOccurs="0"/>

			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="Status" type="xsd:int">
							<xsd:annotation>
								<xsd:documentation>
									Результат операции.
									Возможные значения:
									0 – Документ успешно отозван
									1 – Документ не может быть отозван (например, т.к. уже был проведен)
									2 – При попытке отзыва документа произошла ошибка
									3 – Указанный документ в АБС не найден
									4 – Документ уже был отозван ранее (попытка повторно отозвать ранее отозванный документ)
									5 – Запрос на отзыв в АБС выгружен, результат неизвестен. При этом, запросу присваивается статус «Исполнен».
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CardListBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"></xsd:element>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="1" name="CustomerID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Идентификатор клиента
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="BranchId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор подразделения банка клиента (БИК или код)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="AccNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Номер карточного счета. Для получения списка всех карт передается null
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="CardNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Номер карты. Для получения списка всех карт передается null
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CardListResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BranchId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>

			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="cards" type="bons0:Card" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="Card">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="PROCESSING">
				<xsd:annotation>
					<xsd:documentation>
						Интеграционный код
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="34" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CN">
				<xsd:annotation>
					<xsd:documentation>
						Номер карты
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="19" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="BIC">
				<xsd:annotation>
					<xsd:documentation>
						БИК филиала Банка, где открыт счет клиента.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="9" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="BCN" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Номер основной карты
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CA">
				<xsd:annotation>
					<xsd:documentation>
						Номер счёта
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CRD">
				<xsd:annotation>
					<xsd:documentation>
						Цифровой ISO-код валюты счета.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CUR">
				<xsd:annotation>
					<xsd:documentation>
						Символьный ISO-код валюты счета.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="BD" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата открытия карты.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ED" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата закрытия карты.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="R" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Остаток по карте.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CRLM" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Лимит кредита
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PC" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Процентная ставка по кредиту.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TYPE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Тип карты.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TYPENAME" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Описание типа карты.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="STATUS" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Статус карты
						Принимает значения:
						2 - активна, 3 - просрочена. 4- блокирована, 5 - удалена
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CBLNG" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>
						1: корпоративная, 2: личная.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NNAME" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Имя владельца.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ENAME" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Эмбассированное имя владельца.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="VID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Вид карты
						Принимает значения:
						DEBIT
						CREDIT
						OVERDRAFT
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="EXP" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Срок действия.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SMS" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
						Включена ли услуга смс-информирования по карте.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CROLE" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>
						Роль карты (основная/дополнительная)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CONTRACTID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Код контракта
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CARDHASH" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Хэш карты
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PRODCODE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Код продукта
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PRODNAME" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Наименование карточного продукта
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SMSPHONE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Номер телефона для SMS-информирования по карте
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="GD" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата окончания периода беспроцентного погашения
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ABLOCK" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма заблокированных средств
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ODBT" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма просроченной задолженности
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PRCONREST" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Процент на остаток
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MARK" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Дополнительная информация
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TECHOVER" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Процентная ставка по тех.овердрафту
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CONTRACTSTATUS" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Статус договора:
						0 – Не нарушен
						1 – Нарушен
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CARHOLDERID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Уникальный внешний идентификатор держателя карты
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DCTTYPE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Тип документа, удостоверяющего личность
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DCTNUM" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Номер документа, удостоверяющего личность
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DCTSER" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Серия документа, удостоверяющего личность
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DCTORG" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Орган, выдавший документ, удостоверяющий личность
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DCTDATE" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата документа, удостоверяющего личность
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>


			<xsd:element minOccurs="0" name="OPC" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Процентная ставка за просроченную задолженность
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DBT" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма задолженности по основному долгу
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PRCA" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма начисленных процентов
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PRCO" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма просроченных процентов
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="UCSUT" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата, на которую выводится остаток
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DATELIMIT" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата окончания срока кредитного лимита
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ADUE" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Общая задолженность
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AFINES" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Штрафы, пени
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="APREF" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма льготной задолженности
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ACRED" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма кредитного лимита
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AOWN" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма собственных средств
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CBRate">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="C" type="xsd:string"><xsd:annotation><xsd:documentation>Символьный ISO-код валюты</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CRC" type="xsd:string"><xsd:annotation><xsd:documentation>Цифровой ISO-код валюты</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CRB" type="xsd:string"><xsd:annotation><xsd:documentation>Цифровой ISO-код базовой валюты</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BEG" type="xsd:date"><xsd:annotation><xsd:documentation>Дата начала действия курса</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="END" type="xsd:date"><xsd:annotation><xsd:documentation>Дата окончания действия курса</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SCALE"
			             type="xsd:decimal"><xsd:annotation><xsd:documentation>Масштаб валюты</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="R" type="xsd:decimal"><xsd:annotation><xsd:documentation>Курс</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TYPE" type="xsd:string"><xsd:annotation><xsd:documentation>Тип курса</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CBRatesBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys"
			             type="xsd:string">
			</xsd:element>
			<xsd:element minOccurs="0" name="CustomerID"
			             type="xsd:string">
			</xsd:element>

			<xsd:element minOccurs="0" name="response"
			             type="bons0:CBRatesResponse">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CBRatesResponse">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="rates" type="bons0:CBRate"
			             maxOccurs="unbounded">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CCRatesBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"></xsd:element>
			<xsd:element minOccurs="0" name="SubSys"
			             type="xsd:string">
			</xsd:element>
			<xsd:element minOccurs="0" name="CustomerID"
			             type="xsd:string">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CCRatesResponse">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="rates" type="bons0:CCRate"
			             maxOccurs="unbounded">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CCRate">
		<xsd:sequence>
			<xsd:element minOccurs="1" name="C">
				<xsd:annotation><xsd:documentation>Символьный ISO-код валюты</xsd:documentation></xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="1" name="CRC">
				<xsd:annotation><xsd:documentation>Цифровой код валюты</xsd:documentation></xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="1" name="CB">
				<xsd:annotation><xsd:documentation>Символьный ISO-код базовой валюты</xsd:documentation></xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="1" name="CRB">
				<xsd:annotation><xsd:documentation>Цифровой код базовой валюты</xsd:documentation></xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="1" name="R" type="xsd:decimal">
				<xsd:annotation><xsd:documentation>Курс</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="O">
				<xsd:annotation><xsd:documentation>Тип курса. SELL/BUY</xsd:documentation></xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="SELL"></xsd:enumeration>
						<xsd:enumeration value="BUY"></xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="1" name="SC" type="xsd:decimal">
				<xsd:annotation><xsd:documentation>Масштаб валюты</xsd:documentation></xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CheckBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="0" name="CustomerID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="DocID" type="xsd:string"/>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="BranchId" type="xsd:string"/>
		</xsd:sequence>

	</xsd:complexType>

	<xsd:complexType name="CheckDoc">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="REF" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Референс документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="STATUS" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Статус документа.
						Принимает значения:
						1-принят обработку
						2-исполнен
						3-отказан
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NOTICE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Сообщение банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="TYPE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="VALDATE" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата валютирования</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="operatorName" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>ФИО исполнителя по документу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CheckResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>
			<!-- Хлынов -->
			<xsd:element name="BranchId" type="xsd:string" minOccurs="0"/>

			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element maxOccurs="unbounded" minOccurs="0" name="checkDocs" type="bons0:CheckDoc"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CConvBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="0" name="CustomerID" type="xsd:string"/>

			<xsd:element minOccurs="0" name="CURD" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Символьный ISO-код валюты (дебет - продажа)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CURC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Символьный ISO-код валюты (кредит - покупка)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="D" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата первичного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="N" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер первичного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AD" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма в валюте счета дебета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AC" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма в валюте счета кредита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RATE" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Кросс-курс</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ACD" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Счет дебета (списание)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ACC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Счет кредита (зачисление)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DOCID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Референс платежного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PNAME" type="xsd:string"/>
			<xsd:element minOccurs="0" name="PINN" type="xsd:string"/>
			<xsd:element minOccurs="0" name="COMISSION" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма комиссии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="COMISSIONACC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Счет для списания комиссии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PCARD" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер карты плательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RCARD" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер карты получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CConvResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BranchId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>

			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="REF" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Ссылка на документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="Status" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Статус документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CreditListBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"></xsd:element>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="1" name="CustomerID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Идентификатор клиента
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="BranchId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор подразделения банка клиента (БИК или код)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AccNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						номер счета, по которому требуется получить информацию.
						если требуется получить все кредиты - то параметр остается пустым
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CreditListResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BranchId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>

			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="Loans" type="bons0:CreditLoanRecord" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CreditLoanRecord">
		<xsd:sequence>
			<xsd:element minOccurs="1" name="CRTP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Тип (название) кредита.
						dblink (Name) - название кредита
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="D" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата кредитного договора.
						dblink (ContractDate)- дата договора
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="N" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер договора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DT" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата выдачи кредита.
						dblink (DateStart)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ED" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Срок погашения.
						dblink (DateEnd) - срок закрытия кредита
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Prc" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Процентная ставка.
						dblink (Percents) - процентная ставка по кредиту
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="CUR">
				<xsd:annotation>
					<xsd:documentation>
						Символьный ISO-код валюты.
						dblink (CurrCode) - буквенный код валюты
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CRD">
				<xsd:annotation>
					<xsd:documentation>
						Цифровой ISO-код валюты счета.
						dblink (CurrISO) - буквенный код валюты
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="1" name="A" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма кредита.
						dblink (ContractAmount) - сумма кредита
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="R" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Остаток кредита.
						dblink (Rest) - остаток кредита
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="Acc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Ссудный счет.
						dblink (Account) - Номер счета кредита
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="LoanAccount">
				<xsd:annotation>
					<xsd:documentation>
						номер ссудного счета
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="34"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="RepayAccount">
				<xsd:annotation>
					<xsd:documentation>
						номер счета для погашения кредита</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="34"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="1" name="PrcAmount" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма начисленных процентов.
						dblink (PercentAmount) - сумма уплаты процентов
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PrcTerm" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Срок погашения процентов.
						dblink отсутствует
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="ODA" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма просроченной задолженности.
						dblink (OutstandingAmount) - просроченная задолженность (общая)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="OPA" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма просроченных процентов.
						dblink (outstandingPercent) - просроченная задолженность процентов
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="OPrc" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Проценты на просроченную задолженность.
						dblink отсутствует
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" name="Status" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Статус кредита. 0 - не погашен, 1- погашен, 2-просрочен
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MNP" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Минимальная сумма погашения.
						dblink (MinAmount) - минимальная сумма погашения
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PRCP" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма процентов к уплате.
						dblink отсутствует
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="OFN" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Пеня за несвоевременное исполнение обязательств.
						dblink отсутствует
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FN" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Штраф за факт образования просроченной задолженности.
						dblink (Fine) - штрафы и неустойки
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TSM" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Всего к погашению в очередной платеж.
						dblink (NextPayAmount) - сумма очередного платежа
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PTRM" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						dblink (NextPayDate) - дата очередного платежа
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RefTdn" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Референс кредита.
						dblink отсутствует
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ILPEN" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма начисленных процентов по просроченной задолженности.
						dblink (inqLoanPenalty) - пеня за несвоевременную уплату основного долга
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IIPEN" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма начисленных процентов по просроченным процентам.
						dblink (inqIntPenalty) - пеня за несвоевременную уплату процентов
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ProductCode">
				<xsd:annotation>
					<xsd:documentation>
						код продукта
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="ContractID">
				<xsd:annotation>
					<xsd:documentation>
						Код кредитного договора</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="15"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CurrentDebtOnLoan" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						текущая задолженность по кредиту</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="OverduePrincipal" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						просроченная задолженность основного долга</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AvailAmount" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						доступный остаток по счету</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="OutstandingAmountAll" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						непогашенная стоимость</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RestLoanAccount" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						баланс счета 40817, связанного с ссудным счетом кредита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TypePayment">
				<xsd:annotation>
					<xsd:documentation>
						тип платежа</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="32"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CreditFullCost" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						полная стоимость кредита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CreditTotalAmountDebtPay" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						общая сумма задолженности, подлежащая немедленной уплате</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AccCreditRepayment">
				<xsd:annotation>
					<xsd:documentation>
						счет, с которого производится погашение</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="AccCreditRepayment2">
				<xsd:annotation>
					<xsd:documentation>
						счет для досрочного погашения</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="InsuranceLoanerType">
				<xsd:annotation>
					<xsd:documentation>
						тип страхования заемщика. параметр запроектирован на перспективу и в настоящее время не используется</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="30"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="GracePeriodPayment" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						платеж за Грейс-период. платеж за льготный период уплаты процентов по кредиту</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AccruedPercent" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						начисленные проценты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CreditProductType">
				<xsd:annotation>
					<xsd:documentation>
						тип кредита.
						C_LINE - кредитная линия.
						C_CARD - пластиковые карты.
						C_OTHERS - остальные кредиты.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="15"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CurrentPercent" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						текущая задолженность по процентам</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CurrentLimit" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						сумма доступного лимита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DateDebt" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						дата общей задолженности по кредитному договору</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AmountCommisOtherPay" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						сумма комиссии и прочих платежей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Restructured" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
						признак реструктурированности кредита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CreditPayBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="0" name="CustomerID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="Acc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер ссудного счета клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="response" type="bons0:CreditPayResponse"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CreditPayResponse">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="D" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата начала кредитного договора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DEnd" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата окончания кредитного договора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="N" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер кредитного договора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CUR" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Цифровой ISO-код валюты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Acc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Ссудный счет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="A" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма кредита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Prc" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Процентная ставка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Commis" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Процент комиссии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PaysTA" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Итоговая сумма всех платежей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PaysTAP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Итоговая сумма оплаты основного долга и процентов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PaysTCP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Итоговая сумма оплаты основного долга</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PaysTPrc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Итоговая сумма оплаты процентов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PaysTCommis" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Итоговая сумма оплаты комиссий</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PaysR" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Итоговый остаток</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FactTA" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Итоговая сумма всех фактически произведенных платежей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FactTCP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Итоговая сумма уплаченного основного долга</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FactTPrc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Итоговая сумма уплаченных процентов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FactTCommis" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Итоговая сумма уплаченных комиссий</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FactR" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Остаток задолженности</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FactPay" type="bons0:CreditPayRecord" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>оплата по &quot;факту&quot;</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PlanTA" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Общая сумма оставшихся плановых платежей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PlanTCP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>общая сумма оплаты оставшегося основного долга</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PlanTPrc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>общая сумма оплаты оставшихся процентов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PlanTCommis" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>общая сумма оплаты оставшихся комиссий</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PlanR" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Остаток задолженности</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PlanPay" type="bons0:CreditPayRecord" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>оплата по &quot;плану&quot;</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CreditPayRecord">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="D" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="A" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Общая сумма платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>основной долг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Prc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>проценты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Commis" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Сумма комиссии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="R" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Остаток долга после уплаты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="DepositListBO">
		<xsd:sequence>
			<xsd:element minOccurs="1" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="1" name="CustomerID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="BranchId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор подразделения банка клиента (БИК или код)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="DepositListResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BranchId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>

			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="deposits" type="bons0:Deposit" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="Deposit">
		<xsd:sequence>
			<xsd:element minOccurs="1" name="CA">
				<xsd:annotation>
					<xsd:documentation>
						Номер счёта
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CUR">
				<xsd:annotation>
					<xsd:documentation>
						Символьный ISO-код валюты счета.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element minOccurs="0" name="CRD">
				<xsd:annotation>
					<xsd:documentation>
						Цифровой ISO-код валюты счета.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element minOccurs="1" name="NAME" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Название счета.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="REST" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Остаток по счету.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="DO" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата открытия счета.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="ED" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата окончания договора.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="EDA" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата окончания срока, допускающего пополнение.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="BDW" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата, раньше которой нельзя пополнять депозит.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="LIMIT" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Неснижаемый остаток по счету.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="MA" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Минимальная сумма пополнения счета.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="PERCENT" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Процентная ставка по договору.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="FILIAL" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Код филиала банка, где открыт счет клиента.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element minOccurs="0" name="OA" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Первоначальная сумма вклада.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CID">
				<xsd:annotation>
					<xsd:documentation>
						ID договора.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="15" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CN">
				<xsd:annotation>
					<xsd:documentation>
						Номер договора по вкладу.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="100" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="OPACC">
				<xsd:annotation>
					<xsd:documentation>
						Номер счета, с которого произошло открытие вклада.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="34" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="PERACC">
				<xsd:annotation>
					<xsd:documentation>
						Номер счета выплаты процентов.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="34" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="CLACC">
				<xsd:annotation>
					<xsd:documentation>
						Номер счета для перевода по факту закрытия вклада.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="34" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="PERCN">
				<xsd:annotation>
					<xsd:documentation>
						Номер карты, на которую будут начисляться проценты.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="34" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="PERCENT2" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Действующая процентная ставка.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PC">
				<xsd:annotation>
					<xsd:documentation>
						Код продукта.
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="ISCAP" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
						Возможность капитализации.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="LPD" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Дата последней пролонгации.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ISPA" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
						Наличие автоматической пролонгации.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IABE" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Cумма процентов, которая будет начислена до конца срока.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IAF" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Сумма начисленных процентов.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ADOE" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Cумма депозита на конец срока депозита.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AAFD" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>
						Cумма, доступная для снятия.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SNAME" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Kраткая дополнительная информация.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PERDEP" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>
						Срок вклада в днях.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PERPP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Периодичность уплаты процентов.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RET" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Ставка досрочного расторжения.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ISM" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
						Возможность пополнения вклада.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ISREM" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
						Возможность снятия с вклада.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ISPE" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
						Частичное расходование вклада.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TPS" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						Условия частичного расходования.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PBD" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Возможные сроки частичного расходования: начало периода.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PED" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>
						Возможные сроки частичного расходования: конец периода.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CP" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>
						Возможное количество пролонгаций.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ISPR" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>
						Признак использования прогрессивной процентной ставки.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AccCustBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="0" name="ACC" type="xsd:string"/>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="BranchId" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AccCustResponse">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="0" name="AnsDateTime" type="xsd:dateTime"/>
			<xsd:element minOccurs="0" name="ACC" type="xsd:string"/>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="BranchId" type="xsd:string"/>

			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="CustomerID" type="xsd:string"/>
						<xsd:element minOccurs="0" name="BranchId" type="xsd:string"/>
						<xsd:element minOccurs="0" name="ACC" type="xsd:string"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CardCustBO">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CARD" type="xsd:string" minOccurs="0"/>
			<!-- Хлынов -->
			<xsd:element name="BranchId" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CardCustResponse">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="0" name="AnsDateTime" type="xsd:dateTime"/>
			<xsd:element minOccurs="0" name="CARD" type="xsd:string"/>
			<!-- Хлынов -->
			<xsd:element minOccurs="0" name="BranchId" type="xsd:string"/>

			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="CustomerID" type="xsd:string"/>
						<xsd:element minOccurs="0" name="BranchId" type="xsd:string"/>
						<xsd:element minOccurs="0" name="ACC" type="xsd:string"/>
						<xsd:element minOccurs="0" name="CARD" type="xsd:string"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CustBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="0" name="CustomerID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="ABSCustId"	type="xsd:string">
				<xsd:annotation><xsd:documentation>Код клиента в АБС</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BranchId" type="xsd:string">
				<xsd:annotation><xsd:documentation>Код филиала клиента</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CustResponse">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="0" name="CustomerID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="ABSCustId"	type="xsd:string"/>
			<xsd:element minOccurs="0" name="BranchId" type="xsd:string"/>
			<xsd:element minOccurs="0" name="AnsDateTime" type="xsd:dateTime"/>
			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="NF" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Полное название</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="300"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="NI" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Международное название</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:minLength value="1"/>
									<xsd:maxLength value="255"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="INN" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>ИНН клиента</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="14"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="FX" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Факс</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="EM" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Электронная почта</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="35"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="CTR" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Номер договора на обслуживание</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="CTD" type="xsd:date" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дата договора на обслуживание</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LC" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации: Страна</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="LS" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации: Штат / республика</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="LPT" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации: Тип населенного пункта</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LP" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации: Населенный пункт</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="35"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="LA" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации: Адрес</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="255"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="LZ" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации: Индекс</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="10"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="LR">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации: Район</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="LSTR">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации: Улица</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="35"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="LH">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации: Дом</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="5"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="LB">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации: Корпус</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="5"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="LF">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации: Квартира</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="5"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="IA" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Международный адрес Адрес</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="255"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="PH" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Основной (домашний) телефон</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="15"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="WP" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Рабочий телефон</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="15"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="MP" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Мобильный телефон</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="15"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="SMSP">
							<xsd:annotation>
								<xsd:documentation>Телефон для СМС-сообщений</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="15"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="CS" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Гражданство</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="25"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="PIT" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Тип документа, удостоверяющего личность (ДУЛ)</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="60"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="PN" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Номер паспорта (ДУЛ)</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="PS" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Серия паспорта (ДУЛ)</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="PD" type="xsd:date" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дата выдачи паспорта (ДУЛ)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PP" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Где выдан паспорт (ДУЛ)</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="100"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="SC" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Код подразделения</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="BD" type="xsd:date" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дата рождения физ. лица</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BP" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Место рождения физ. лица</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="100"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="GN" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Пол</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="Ж"/>
									<xsd:enumeration value="М"/>
									<xsd:enumeration value="F"/>
									<xsd:enumeration value="M"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="PC" type="xsd:decimal" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Допустимые для использования клиентом банковские продукты</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="BRANCH_ID" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Идентификатор подразделения банка клиента (БИК или код)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="PupayBO">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<!-- Хлынов -->
			<xsd:element name="BranchId" type="xsd:string"/>
			<!-- Хлынов -->
			<xsd:element name="FlagComiss" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>
						Признак расчета комиссии операции.
						Принимает  значения:
						0: Не рассчитывать
						1: Рассчитывать.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PUREF" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор услуги в АБС.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UTILITY" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование услуги.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="D" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата документа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXP" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата окончания действия карты плательщика.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="N">
				<xsd:annotation>
					<xsd:documentation>Номер документа.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="A" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма платежа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="COMMIS" type="xsd:decimal" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Комиссия.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RCOMMIS" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма комиссии с получателя платежа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CUR">
				<xsd:annotation>
					<xsd:documentation>ISO-код валюты.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CRD">
				<xsd:annotation>
					<xsd:documentation>Валюта.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PINN">
				<xsd:annotation>
					<xsd:documentation>ИНН плательщика.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="12"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PNAME">
				<xsd:annotation>
					<xsd:documentation>Наименование плательщика.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PACC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Счет плательщика.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CN" type="xsd:string" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Номер карты плательщика.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PIDD" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип документа, удостоверяющего личность (ДУЛ).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PIDN" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер и серия ДУЛ.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PIDI" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Дата и место выдачи ДУЛ.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RINN" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>ИНН получателя.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RNAME" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование получателя платежа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RTYP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип получателя платежа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RACC">
				<xsd:annotation>
					<xsd:documentation>Номер счета получателя платежа.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="RBBIC" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>БИК банка получателя.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="RBNAME" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование банка получателя.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DOCID" type="xsd:string" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Референс платежного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GROUND" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Основание платежа для выгрузки документа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="COMMISACC" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Счет для списания комиссии.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PBBIC" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>БИК банка плательщика.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PBNAME" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование банка плательщика.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PBCORRACC" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Корсчет банка клиента.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PUID" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор услуги в системе.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DOCSTATUS" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Статус документа в системе.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SDREF" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Референс документа из сторонней системы.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PSPUREF" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор услуги в ПС.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PACKETCHECK" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Шаблон проверки документа в системе Киберплат.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PACKETPAY" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Шаблон оплаты документа в системе Киберплат.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ADDITIONAL" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Значение атрибута BSRequest/ADDITIONAL запроса ЧК.Дополнительные параметры платежа.
						-- Ком:	Несколько строк.
						-- Каждая новая строка должна начинаться стартовым символом (newline - перевод строки, LF, ASCII код 10) и заканчиваться символом конца записи (возврат каретки, CR, ASCII 13). Значение атрибута всегда заключено в кавычки, соответственно если внутри кавычек встретится точка с запятой, то она будет проигнорирована. Если в значении атрибута встречаются кавычки, то они должны быть удвоены. Пример: значение атрибута FIELDVALUE - ОАО "Банк" передается как "ОАО ""Банк"""
						-- Первая строка: Перечислены имена атрибутов тега ADDITIONAL, разделенные символом «;». Данная строка задает последовательность данных в остальных строках.
						-- Каждая последующая: Перечислены значения атрибутов в той последовательности, в которой их имена идут в первой строке.
						-- FIELDNAME	Наименование параметра
						-- FIELDID	Идентификатор параметра
						-- FIELDVALUE	Значение параметра
						-- ABSFIELDID	Идентификатор параметра в АБС
						-- PSID	Идентификатор параметра в ПС
						-- Маска:	FIELDNAME;FIELDID;FIELDVALUE;ABSFIELDID;PSID
						-- “FIELDNAME_V1”;”FIELDID_V1”;”FIELDVALUE_V1”;”ABSFIELDID_V1”;”PSID_V1”
						-- “FIELDNAME_V2”;”FIELDID_V2”;”FIELDVALUE_V2”;”ABSFIELDID_V2”;”PSID_V2”
						-- …
						-- “FIELDNAME_VN”;”FIELDID_VN”;”FIELDVALUE_VN”;”ABSFIELDID_VN”;”PSID_VN”
						-- Пример:	FIELDNAME;FIELDID;FIELDVALUE;ABSFIELDID;PSID
						-- "Номер телефона";"1";"9172345364";"";""
						-- “Email”;”2”;”bss@mail.com”;”111”;”00011”
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PERIOD" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>Периодичность цифрами (10 дней например).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UNIT" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Единицы измерения периодичности – W, M, D, Y.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BDATE" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Начальная дата.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EDATE" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Конечная дата.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

