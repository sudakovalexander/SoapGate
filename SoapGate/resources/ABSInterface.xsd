													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="sign" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Подпись</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="type" type="xsd:string">
