			<xsd:element name="GND" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Назначение платежа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Рублевое РКО - Виды операций, поле Код. Значение вида '01' передаем как '1'</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ST" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Рублевое РКО - Способы отправки платежа, поле Наименование</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PRIORITY" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Рублевое РКО - Очередности платежа, поле Код</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="PlatporResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>

			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="REF" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Ссылка на документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="Status" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Статус документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="StmBO">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Уникальный код запроса от системы SubSys</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование системы, сформировавшей запрос</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BranchId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код филиала клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BDate" type="xsd:date" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Дата начала периода выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EDate" type="xsd:date" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Дата окончания периода выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Account" type="xsd:string" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Счет клиента, по которому запрашивается выписка - в случае выписки по счету</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CardNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер карты, в случае выписки по карте</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="StmResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="EDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="Account" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="BranchId" type="xsd:string" minOccurs="0"/>

			<xsd:element name="response" minOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="CUR" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Символьный ISO код валюты счета</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="NAME" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Название счета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="REST" type="xsd:decimal" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Остаток на счете на текущую дату</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IB" type="xsd:decimal" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Входящий остаток по счету на начало периода</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="OB" type="xsd:decimal" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Исходящий остаток по счету за запрашиваемый период</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LODate" type="xsd:date" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дата последней операции по счёту</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!-- Хлынов -->
						<xsd:element name="DEB" type="xsd:decimal" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Сумма оборота по дебету, за указанный период в валюте счета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!-- Хлынов -->
						<xsd:element name="CRE" type="xsd:decimal" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Сумма оборота по кредиту за указанный период в валюте счета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="statements" type="bons0:StatementDoc" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="StatementDoc">
		<xsd:sequence>
			<xsd:element name="EXT_DOC_ID" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation><xsd:documentation>внешний id документа в АБС</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="D" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата первичного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PD" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата обработки документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="N">
				<xsd:annotation>
					<xsd:documentation>Номер первичного документа</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="A" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="G" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Назначение платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CT" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>CharType - признак ДТ/КТ. 1 - дебет, 0 - кредит</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Рублевое РКО - Виды операций, поле Код. Значение вида '01' передаем как '1'</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SYMBOL" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Символ кассового планирования (СКП), используется для вида операции 04 (объявление на взнос наличными)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ST" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Рублевое РКО - Способы отправки платежа, поле Наименование</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PRIORITY" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Рублевое РКО - Очередности платежа, поле Код</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PKPP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>PayerKPP - КПП плательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RKPP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>ReceiverKPP - КПП получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Налоговые поля -->
			<xsd:element name="OKATO" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код ОКАТО.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TS" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Показатель статуса налогового платежа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REASON" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Показатель основания платежа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Показатель налогового периода.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TN" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Показатель номера документа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TD" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Дата налогового документа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TT" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Показатель типа платежа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CBCCODE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код КБК.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UIN" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор начислений (УИН)</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element name="AO">
				<xsd:annotation>
					<xsd:documentation>Номер счета владельца</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="NO" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование владельца</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="INNO" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ИНН владельца</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="KPPO" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>КПП владельца</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BIKO">
				<xsd:annotation>
					<xsd:documentation>БИК банка владельца</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BNAMEO">
				<xsd:annotation>
					<xsd:documentation>Наименование банка владельца</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BCAO">
				<xsd:annotation>
					<xsd:documentation>Корсчет банка владельца</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element name="AC">
				<xsd:annotation>
					<xsd:documentation>Номер счета контрагента</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="NC" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование контрагента</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="INNC" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ИНН контрагента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="KPPC" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>КПП контрагента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BIKC">
				<xsd:annotation>
					<xsd:documentation>БИК банка контрагента</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BNAMEC">
				<xsd:annotation>
					<xsd:documentation>Наименование банка контрагента</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BCAC">
				<xsd:annotation>
					<xsd:documentation>Корсчет банка контрагента</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<!-- Хлынов -->
			<xsd:element name="SDATE" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата создания документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element name="TDATE" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата проведения документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element name="AOPER" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма документа в валюте операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element name="OPCUR" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Валюта операции. передается как код ISO</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="3"/>
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element name="CRDN" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element name="HOLD" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак холдирования суммы</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element name="CLINAME" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Полное имя клиента (ФИО)</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element name="MCC" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>MCC код</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="RDATE" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата поступления в банк плательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов -->
			<xsd:element name="STYPE" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Вид отправки</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ExecDateBank" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата списания со счета плательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RegDateBank" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата	поступления	документа в банк плательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StatDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата исполнения документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов-->
			<xsd:element name="BPO" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Адрес банка владельца выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Хлынов-->
			<xsd:element name="BPC" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Адрес банка контрагента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="operatorName" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>ФИО исполнителя по документу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="LastOperationBO">
		<xsd:sequence>
			<xsd:element name="reqId" type="xsd:string" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Уникальный код запроса от системы subSys</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="subSys" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование системы, сформировавшей запрос</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="customerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="branchId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код филиала банка, в котором обслуживается клиент</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="maxNumber" type="xsd:integer" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Максимальное количество заказываемых операций</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="isDirect" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип документов: 0 – входящие, 1 – исходящие, не указано – все</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="afterDocId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код документа в АБС после которого нужно возвращать maxNumber документов. Если не указано, то с самого последнего</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="accounts" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Список счетов клиента, по которым запрашиваются последние операции</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="account" minOccurs="1" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									Номер счёта
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="40" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="LastOperationResponse">
		<xsd:sequence>
			<xsd:element name="reqId" type="xsd:string" minOccurs="1"/>
			<xsd:element name="subSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="customerId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="branchId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="maxNumber" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="isDirect" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="afterDocId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ansDateTime" type="xsd:dateTime" minOccurs="0"/>

			<xsd:element name="response" minOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="transaction" type="bons0:TransactionDoc" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="TransactionDoc">
		<xsd:sequence>
			<xsd:element name="extDocId" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор документа в АБС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="docId" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Идентификатор первичного дебетового документа в iSimple, если документ из другой системы или кредитовый, то не заполняется</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="docDate" type="xsd:date" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Дата первичного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="docNum" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Номер первичного документа</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="isDirect" type="xsd:integer" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>признак ДТ/КТ: 1 – списание, 0 – пополнение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fromAmount" type="xsd:decimal" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Сумма списания в валюте списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fromCur" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Буквенный ISO-код валюты счета списания</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="3"/>
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:element name="fromAcc" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Номер счета списания</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fromName" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Наименование отправителя (полностью)</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fromINN" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>ИНН отправителя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="12"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fromKPP" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>КПП отправителя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fromBIK" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>БИК банка отправителя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fromNameBank" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Наименование банка отправителя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fromPlaceBank" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Населенный пункт банка отправителя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="fromCorrBank" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Корсчет банка отправителя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="toAmount" type="xsd:decimal" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Сумма зачисления в валюте зачисления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="toCur" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Буквенный ISO-код валюты счета зачисления</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="3"/>
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="toAcc" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Номер счета зачисления</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="toName" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Наименование получателя (полностью)</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="toINN" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>ИНН получателя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="12"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="toKPP" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>КПП получателя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="toBIK" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>БИК банка получателя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="toNameBank" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Наименование банка получателя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="toPlaceBank" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Населенный пункт банка получателя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="toCorrBank" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Корсчет банка получателя</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="purpose" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Назначение платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="priority" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Очередность платежа</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="sendType" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Вид отправки (электронно, срочно и т.д)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="docType" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Тип документа  (1- ПП, 17-БО, 09- МО и т.д). </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="docUIN" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор начислений (УИН)</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Налоговые поля -->
			<xsd:element name="taxState" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Статус составителя (101)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="KBK" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>КБК (104)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OKTMO" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>ОКТМО (105)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="taxPurpose" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Основание налогового платежа (106)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="taxPeriod" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Налоговый период (107)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="taxNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер налогового платежа (108)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="taxDate" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Дата налогового платежа (109)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="taxType" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Показатель типа платежа (110)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="registerDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата	поступления	документа в свой банк</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="registerDateBank" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата поступления документа в сторонний банк (для внутрибанковского документа или при отсутствии информации устанавливается равным registerDate)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="executeDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата проведения документа в своем банке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="executeDateBank" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата проведения документа в стороннем банке банк (для внутрибанковского документа или при отсутствии информации устанавливается равным executeDate)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="docOperatorName" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>ФИО исполнителя документа (сотрудника банка)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="LastOperationInfoBO">
		<xsd:sequence>
			<xsd:element name="reqId" type="xsd:string" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Уникальный код запроса от системы subSys</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="subSys" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование системы, сформировавшей запрос</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="customerId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="branchId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код филиала банка, в котором обслуживается клиент</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="documents" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Список идентификаторов в АБС документов клиента</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="id" minOccurs="1" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									Идентификатор документа в АБС
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="40" />
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="LastOperationInfoResponse">
		<xsd:sequence>
			<xsd:element name="reqId" type="xsd:string" minOccurs="1"/>
			<xsd:element name="subSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="customerId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="branchId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ansDateTime" type="xsd:dateTime" minOccurs="0"/>

			<xsd:element name="response" minOccurs="1">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="transaction" type="bons0:TransactionDoc" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="OpenDepositBO">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Уникальный референс документа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DOCDATE" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата распоряжения.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DOCNUMBER">
				<xsd:annotation>
					<xsd:documentation>Номер распоряжения.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="15"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AMOUNTDEBET" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма списания в валюте счета списания.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AMOUNTCREDIT" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма зачисления в валюте депозита.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EXCHANGERATE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Курс обмена.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DESCRIPTION" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Назначение платежа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ACCNUMBER">
				<xsd:annotation>
					<xsd:documentation>Номер счёта, с которого перечисляются средства на депозит.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="34"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CARDNUMBER">
				<xsd:annotation>
					<xsd:documentation>Номер карты, с которой перечисляются средства на депозит.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="16"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PROGCODE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип депозита.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BRANCHCODE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код филиала счёта, с которого перечисляются средства на депозит.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PROGVALIDDAY" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Срок депозита в днях.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PERCENTVALUE" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Процентная ставка.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PROGCURRISOCODE">
				<xsd:annotation>
					<xsd:documentation>Буквенный ISO-код валюты перевода.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PROGCURRCODE">
				<xsd:annotation>
					<xsd:documentation>Цифровой код валюты перевода.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
