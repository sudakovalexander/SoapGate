	<xsd:complexType name="ContractDepositCancelResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="BranchId" type="xsd:string" minOccurs="0"/>
			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="Status" type="xsd:int">
							<xsd:annotation>
								<xsd:documentation>Результат операции.
									-- Возможные значения:
									-- 0 – Документ успешно отозван
									-- 1 – Документ не может быть отозван (например, т.к. уже был проведен)
									-- 2 – При попытке отзывадокумента произошла ошибка
									-- 3 – Указанный документ в АБС не найден
									-- 4 – Документ уже был отозван ранее (попытка повторно отозвать ранее отозванный документ)
									-- 5 – Запрос на отзыв в АБС выгружен, результат неизвестен.
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="Mails2ClientListRequest"/>

	<xsd:complexType name="Mails2ClientListResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="mail2Client" minOccurs="0" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="corporateId" minOccurs="0" maxOccurs="unbounded" type="xsd:string">
										<xsd:annotation>
											<xsd:documentation>Идентификатор клиента в АБС</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="groupId" minOccurs="0" maxOccurs="unbounded" type="xsd:string">
										<xsd:annotation>
