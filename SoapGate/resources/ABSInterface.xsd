	<xsd:complexType name="PaymentScheduleResponse">
		<xsd:sequence>
			<xsd:element name="reqID" type="xsd:string" minOccurs="1"/>
			<xsd:element name="SubSys" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustomerID" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnsDateTime" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="response" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="BEGIN_DATE" type="xsd:date">
							<xsd:annotation><xsd:documentation>дата начала кредитного договора</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="END_DATE" type="xsd:date">
							<xsd:annotation><xsd:documentation>дата окончания кредитного договора</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="CONTRACT_NUM" type="xsd:string">
							<xsd:annotation><xsd:documentation>номер кредитного договора</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="ACCOUNT" type="xsd:string">
							<xsd:annotation><xsd:documentation>ссудный счет</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="CUR" type="xsd:string">
							<xsd:annotation><xsd:documentation>цифровой код валюты</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="AMOUNT" type="xsd:decimal">
							<xsd:annotation><xsd:documentation>сумма кредита</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="PRCNT" type="xsd:decimal">
							<xsd:annotation><xsd:documentation>процентная ставка</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="COMMIS" type="xsd:decimal">
							<xsd:annotation><xsd:documentation>процент комиссии</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="TOTAL_AMOUNT" type="xsd:decimal">
							<xsd:annotation><xsd:documentation>итоговая сумма платежей по кредиту</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="TOTAL_PRINCIPIAL_PERCENT" type="xsd:decimal">
							<xsd:annotation><xsd:documentation>итоговая сумма кредита и процентов по кредитному договору</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="TOTAL_PRINCIPIAL" type="xsd:decimal">
							<xsd:annotation><xsd:documentation>итоговая сумма оплаты основного долга</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="TOTAL_PERCENT" type="xsd:decimal">
							<xsd:annotation><xsd:documentation>итоговая сумма оплаты процентов</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="TOTAL_COMMIS" type="xsd:decimal">
							<xsd:annotation><xsd:documentation>итоговая сумма оплаты комиссий</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="TOTAL_REST" type="xsd:decimal">
							<xsd:annotation><xsd:documentation>общий остаток задолженности</xsd:documentation></xsd:annotation>
						</xsd:element>
						<xsd:element name="FACT_PAYS">
							<xsd:annotation><xsd:documentation>фактические платежи - итого</xsd:documentation></xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="AMOUNT" type="xsd:decimal">
										<xsd:annotation><xsd:documentation>общая сумма фактических платежей</xsd:documentation></xsd:annotation>
									</xsd:element>
									<xsd:element name="PRINCIPIAL" type="xsd:decimal">
										<xsd:annotation><xsd:documentation>в т.ч. общая сумма оплаты основного долга</xsd:documentation></xsd:annotation>
									</xsd:element>
									<xsd:element name="PRCNT" type="xsd:decimal">
										<xsd:annotation><xsd:documentation>в т.ч. общая сумма оплаты процентов</xsd:documentation></xsd:annotation>
									</xsd:element>
									<xsd:element name="COMMIS" type="xsd:decimal">
										<xsd:annotation><xsd:documentation>в т.ч. общая сумма оплаты комиссий</xsd:documentation></xsd:annotation>
									</xsd:element>
									<xsd:element name="REST" type="xsd:decimal">
										<xsd:annotation><xsd:documentation>общий остаток задолженности</xsd:documentation></xsd:annotation>
									</xsd:element>
									<xsd:element name="PAYS" maxOccurs="unbounded">
										<xsd:annotation><xsd:documentation>фактические платежи</xsd:documentation></xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="PAY_DATE" type="xsd:date">
													<xsd:annotation><xsd:documentation>дата платежа</xsd:documentation></xsd:annotation>
												</xsd:element>
												<xsd:element name="AMOUNT" type="xsd:decimal">
													<xsd:annotation><xsd:documentation>сумма платежа</xsd:documentation></xsd:annotation>
												</xsd:element>
												<xsd:element name="PRINCIPIAL" type="xsd:decimal">
													<xsd:annotation><xsd:documentation>в т.ч. основной долг</xsd:documentation></xsd:annotation>
												</xsd:element>
												<xsd:element name="PRCNT" type="xsd:decimal">
													<xsd:annotation><xsd:documentation>в т.ч. проценты</xsd:documentation></xsd:annotation>
												</xsd:element>
												<xsd:element name="COMMIS" type="xsd:decimal">
													<xsd:annotation><xsd:documentation>сумма комиссии</xsd:documentation></xsd:annotation>
												</xsd:element>
												<xsd:element name="REST" type="xsd:decimal">
													<xsd:annotation><xsd:documentation>остаток</xsd:documentation></xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="PLAN_PAYS">
							<xsd:annotation><xsd:documentation>плановые платежи - итого</xsd:documentation></xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="AMOUNT" type="xsd:decimal">
										<xsd:annotation><xsd:documentation>общая сумма плановых платежей</xsd:documentation></xsd:annotation>
									</xsd:element>
									<xsd:element name="PRINCIPIAL" type="xsd:decimal">
										<xsd:annotation><xsd:documentation>в т.ч. общая сумма оплаты основного долга</xsd:documentation></xsd:annotation>
									</xsd:element>
									<xsd:element name="PRCNT" type="xsd:decimal">
										<xsd:annotation><xsd:documentation>в т.ч. общая сумма оплаты процентов</xsd:documentation></xsd:annotation>
									</xsd:element>
									<xsd:element name="COMMIS" type="xsd:decimal">
										<xsd:annotation><xsd:documentation>в т.ч. общая сумма оплаты комиссий</xsd:documentation></xsd:annotation>
									</xsd:element>
									<xsd:element name="REST" type="xsd:decimal">
										<xsd:annotation><xsd:documentation>общий остаток задолженности</xsd:documentation></xsd:annotation>
									</xsd:element>
									<xsd:element name="PAYS" maxOccurs="unbounded">
										<xsd:annotation><xsd:documentation>плановые платежи</xsd:documentation></xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="PAY_DATE" type="xsd:date">
													<xsd:annotation><xsd:documentation>дата платежа</xsd:documentation></xsd:annotation>
												</xsd:element>
												<xsd:element name="AMOUNT" type="xsd:decimal">
													<xsd:annotation><xsd:documentation>сумма платежа</xsd:documentation></xsd:annotation>
												</xsd:element>
												<xsd:element name="PRCNT" type="xsd:decimal">
													<xsd:annotation><xsd:documentation>в т.ч. проценты</xsd:documentation></xsd:annotation>
												</xsd:element>
												<xsd:element name="COMMIS" type="xsd:decimal">
													<xsd:annotation><xsd:documentation>сумма комиссии</xsd:documentation></xsd:annotation>
												</xsd:element>
												<xsd:element name="REST" type="xsd:decimal">
													<xsd:annotation><xsd:documentation>остаток</xsd:documentation></xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="ContractDepositCancelRequest">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="0" name="CustomerID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="CancelDocRef" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Референс отзываемого документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="D" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="N" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер платежного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CANCELDOCID" type="xsd:string">
				<xsd:annotation>
