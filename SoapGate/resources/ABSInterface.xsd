							</xsd:annotation>
						</xsd:element>

						<xsd:element minOccurs="0" name="Resident" type="bons0:logical">
							<xsd:annotation>
								<xsd:documentation>Признак резидента (1 - резидент;0 - не резидент)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="SmsInformOn" type="bons0:logical">
							<xsd:annotation>
								<xsd:documentation>Признак услуги SMS информирования (1 - включена;0 - отключена)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="logical">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="CardAccBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys"
			             type="xsd:string">
			</xsd:element>
			<xsd:element minOccurs="0" name="CustomerID"
			             type="xsd:string">
			</xsd:element>
			<xsd:element minOccurs="0" name="CUR" type="xsd:string"/>
			<xsd:element minOccurs="0" name="D" type="xsd:date"/>
			<xsd:element minOccurs="0" name="VD" type="xsd:dateTime"/>
			<xsd:element minOccurs="0" name="N" type="xsd:string"/>
			<xsd:element minOccurs="0" name="A" type="xsd:decimal"/>
			<xsd:element minOccurs="0" name="ACD" type="xsd:string"/>
			<xsd:element minOccurs="0" name="CRD" type="xsd:string"/>
			<xsd:element minOccurs="0" name="ACC" type="xsd:string"/>
			<xsd:element minOccurs="0" name="CRC" type="xsd:string"/>
			<xsd:element minOccurs="0" name="GND" type="xsd:string"/>
			<xsd:element minOccurs="0" name="DOCID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="DEXP" type="xsd:date"/>
			<xsd:element minOccurs="0" name="CEXP" type="xsd:date"/>
			<xsd:element minOccurs="0" name="PINN" type="xsd:string"/>
			<xsd:element minOccurs="0" name="PNAME" type="xsd:string"/>
			<xsd:element minOccurs="0" name="PIDD" type="xsd:string"/>
			<xsd:element minOccurs="0" name="PIDN" type="xsd:string"/>
			<xsd:element minOccurs="0" name="PBBIK" type="xsd:string"/>
			<xsd:element minOccurs="0" name="PBCA" type="xsd:string"/>
			<xsd:element minOccurs="0" name="PBTYP" type="xsd:string"/>
			<xsd:element minOccurs="0" name="PBNAME"
			             type="xsd:string">
			</xsd:element>
			<xsd:element minOccurs="0" name="PBP" type="xsd:string"/>
			<xsd:element minOccurs="0" name="response"
			             type="bons0:CardAccResponse">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CardAccResponse">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="REF" type="xsd:string"/>
			<xsd:element minOccurs="0" name="Status"
			             type="xsd:string">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="PDCBO">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="reqID" type="xsd:string"/>
			<xsd:element minOccurs="0" name="SubSys" type="xsd:string"/>
			<xsd:element minOccurs="0" name="CustomerID" type="xsd:string"/>

			<xsd:element name="CUR">
				<xsd:annotation>
					<xsd:documentation>Символьный ISO-код валюты.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CRD">
				<xsd:annotation>
					<xsd:documentation>Цифровой ISO-код валюты.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="D" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата первичного документа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="N">
				<xsd:annotation>
					<xsd:documentation>Номер первичного документа.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="A" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>.Сумма.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PNAME">
				<xsd:annotation>
					<xsd:documentation>Наименование плательщика.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PACC">
				<xsd:annotation>
					<xsd:documentation>Счет плательщика.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PIDD" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип документа, удостоверяющего личность.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PIDN" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер и серия ДУЛ.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PIDI" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Дата и место выдачи ДУЛ.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PBBIK">
				<xsd:annotation>
					<xsd:documentation>БИК банка плательщика в ЦБ.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PBCA">
				<xsd:annotation>
					<xsd:documentation>Корсчет банка плательщика в ЦБ.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PBTYP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип банка плательщика.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PBNAME">
				<xsd:annotation>
					<xsd:documentation>Наименование банка плательщика.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PBPT" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Краткое название типа населенного пункта Банка клиента.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PBP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Название населенного пункта Банка клиента.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RNAME">
				<xsd:annotation>
					<xsd:documentation>Наименование бенефициара.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="RACC">
				<xsd:annotation>
					<xsd:documentation>Счет бенефициара.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="RBCA" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>В настоящее время не используется.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="RBNAME" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Название банка посредника.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RBP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Местонахождение банка посредника (населенный пункт).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BBCCODE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код страны банка бенефициара.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RBBSWIFT">
				<xsd:annotation>
					<xsd:documentation>Код SWIFT посредника.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BBSWIFT">
				<xsd:annotation>
					<xsd:documentation>Код SWIFT бенефициара.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BBCA">
				<xsd:annotation>
					<xsd:documentation>Корсчет банка бенефициара в банке посреднике.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BBNAME">
				<xsd:annotation>
					<xsd:documentation>Название банка бенефициара.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BBP">
				<xsd:annotation>
					<xsd:documentation>Местонахождение банка бенефициара (населенный пункт).</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BBADDR">
				<xsd:annotation>
					<xsd:documentation>Адрес банка бенефициара.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="RCCODE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код страны бенефициара.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RP">
				<xsd:annotation>
					<xsd:documentation>Местонахождение (населенный пункт) бенефициара.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="RADDR">
				<xsd:annotation>
					<xsd:documentation>Адрес бенефициара.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="RBCCODE" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код страны банка-посредника.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RBADDR" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Адрес банка посредника.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="COMMISSION">
				<xsd:annotation>
					<xsd:documentation>Тип комиссии («BEN», «OUR», «SHA»).</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="BEN"/>
						<xsd:enumeration value="OUR"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CACC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Счет для списания комиссии.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DOCID" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Референс платежного документа – уникальный идентификатор документа в ЧК.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OPERCODE">
				<xsd:annotation>
					<xsd:documentation>Код вида валютной операции.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="25"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PAYPURP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>назначение платежа.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="response" type="bons0:PDCResponse"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="PDCResponse">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="REF" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Ссылка на документ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Status" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Статус документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

</xsd:schema>